# Ephemeral Environment Configuration

# AWS Configuration (uses LocalStack endpoint for local dev)
EPHEMERAL_AWS_ENDPOINT_URL=http://localhost:4566
EPHEMERAL_AWS_REGION=us-east-1

# SQS Queues
EPHEMERAL_SQS_QUEUE_HIGH=jobs-high.fifo
EPHEMERAL_SQS_QUEUE_NORMAL=jobs-normal.fifo
EPHEMERAL_SQS_QUEUE_LOW=jobs-low.fifo

# DynamoDB Tables
EPHEMERAL_DYNAMODB_TABLE=jobs
EPHEMERAL_DYNAMODB_RATE_LIMIT_TABLE=rate-limits

# S3 Bucket
EPHEMERAL_S3_BUCKET=job-artifacts

# ECS (for production)
EPHEMERAL_ECS_CLUSTER=ephemeral-agents
EPHEMERAL_ECS_TASK_DEFINITION=agent-task

# Rate Limiting
EPHEMERAL_RATE_LIMIT_PER_MINUTE=10

# Job Configuration
EPHEMERAL_JOB_TIMEOUT_SECONDS=600

# LocalStack credentials (any values work)
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test
